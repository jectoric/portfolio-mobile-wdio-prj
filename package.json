{
  "name": "portfolio-mobile-wdio-project",
  "author": "Yevhenii Marchenko",
  "version": "1.0.0",
  "description": "This is an example mobile WDIO Apiium project which shows the level of my automation testing skills",
  "scripts": {
    "eslint:validate": "eslint .",
    "eslint:fix": "eslint . --color --fix --quiet",
    "report:allure:open": "allure open",
    "run-all-autotests": "wdio config/webdriver/wdio.debug.conf.ts --suite google_translate_all",
    "run-functional-autotests": "wdio config/webdriver/wdio.debug.conf.ts --suite google_translate_functional",
    "run-non-functional-autotests": "wdio config/webdriver/wdio.debug.conf.ts --suite google_translate_non_functional"
  },
  "keywords": [
    "WebdriverIO",
    "Appium",
    "Mobile",
    "Google Translate",
    "Android",
    "Allure"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/base-64": "^1.0.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.13.0",
    "@types/webdriverio": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@wdio/allure-reporter": "^8.38.0",
    "@wdio/appium-service": "^8.38.0",
    "@wdio/cli": "^8.38.1",
    "@wdio/local-runner": "^8.38.0",
    "@wdio/mocha-framework": "^8.38.0",
    "@wdio/spec-reporter": "^8.38.0",
    "@wdio/types": "^8.37.0",
    "allure-commandline": "^2.29.0",
    "appium-uiautomator2-driver": "^3.5.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-wdio": "^8.37.0",
    "mocha": "^10.4.0",
    "path": "^0.12.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
    }
}
